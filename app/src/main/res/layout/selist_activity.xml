<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_main1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/vvv"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin" >
    <EditText
        android:id="@+id/search11"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:hint="Enter the person's name "
        android:layout_marginBottom="@dimen/cardMarginVertical"
        android:layout_marginLeft="@dimen/cardMarginHorizontal"
        android:layout_marginRight="@dimen/cardMarginHorizontal"
        android:layout_marginTop="@dimen/cardMarginVertical"
        />
    <GridView
        android:layout_marginTop="@dimen/_90sdp"
        android:background="@drawable/vvv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gridView1"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:columnWidth="100dp"
        android:gravity="center"
        android:numColumns="auto_fit" />
    <Button
        android:id="@+id/view_button111"
        android:layout_width="@dimen/_60sdp"
        android:layout_height="40dp"
        android:textColor="#ffffff"
        android:textSize="14dp"
        android:text="Search"

        android:background="@drawable/vv"
        android:layout_marginBottom="@dimen/cardMarginVertical"
        android:layout_marginLeft="@dimen/_30sdp"
        android:layout_marginRight="@dimen/_30sdp"
        android:layout_marginTop="@dimen/_50sdp"
        />
</RelativeLayout>